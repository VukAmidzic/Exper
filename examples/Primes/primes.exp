/* This script generates prime numbers
in range [1, n)
*/

n := 0;
scan(n);

i := 0;
curr := 2;

while (i < n) {
    tmp := 2;
    divs := 0;
    
    while (tmp < curr) {
        if (curr % tmp = 0) {divs := divs + 1;};
        
        tmp := tmp + 1;
    };

    if (divs = 0) {
        print(curr);
    };
    
    i := i + 1;
    curr := curr + 1;
};
