/*  This script creates an array of 
    n input elements and outputs
    square of every element
*/

n := 0;
scan(n);

arr[] := [n; 0];

i := 0;
while (i < n) {
    input := 0;
    scan(input);
    
    arr[i] := input * input;
    i := i + 1;
};

i := 0;
while (i < n) {
    print(arr[i]);
    i := i + 1;
};
